{
  "name": "angular1-template",
  "version": "0.1.0",
  "description": "Angular 1 template with protractor tests, coverage and github page",
  "main": "index.js",
  "scripts": {
    "instrument": "./node_modules/.bin/istanbul instrument . --output ../instrumented --preserve-comments",
    "dev": "lite-server",
    "test": "./node_modules/.bin/istanbul test  --include-all-sources ./node_modules/.bin/protractor protractor.config.js"
  },
  "devDependencies": {
    "lite-server": "*",
    "webdriver-manager": "*",
    "jasmine-core": "*",
    "jasmine-node": "*",
    "jasmine-spec-reporter": "^2.4.0",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.4",
    "istanbul": "*",
    "protractor": "*",
    "protractor-istanbul-plugin": "*",
    "codecov": "*"
  }
}
